<%= nav_bar fixed: :top, brand: 'Docket', responsive: true do %>
		<% if current_owner %>
			<%= menu_group do %>
				<%= menu_item 'New Shop', new_owner_shop_path(current_owner) %>
			<% end %>
			<%= menu_group pull: :right do %>
				<%= drop_down current_owner.email do %>
					<%= menu_item 'Edit Profile', edit_owner_registration_path %>
					<%= menu_item "Log Out", destroy_owner_session_path, method: :delete %>
				<% end %>
			<% end %>
		<% else %>
			<%= menu_group pull: :right do %>
				<%= menu_item "Sign Up", new_owner_registration_path %>
				<%= menu_item "Sign In", new_owner_session_path %>
			<% end %>
		<% end %>
<% end %>
